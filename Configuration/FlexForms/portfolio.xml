<T3DataStructure>
	<sheets>
		<!-- General -->
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:trb_pi_portfolio/Resources/Private/Language/locallang_db.xlf:flex.options</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<switchableControllerActions>
						<TCEforms>
							<label>LLL:EXT:trb_pi_portfolio/Resources/Private/Language/locallang_db.xlf:flex.mode</label>
							<onChange>reload</onChange>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:trb_pi_portfolio/Resources/Private/Language/locallang_db.xlf:flex.list</numIndex>
										<numIndex index="1">Portfolio->list;Portfolio->detail</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:trb_pi_portfolio/Resources/Private/Language/locallang_db.xlf:flex.slider</numIndex>
										<numIndex index="1">Portfolio->slider;Portfolio->detail</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:trb_pi_portfolio/Resources/Private/Language/locallang_db.xlf:flex.detail</numIndex>
										<numIndex index="1">Portfolio->detail</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</switchableControllerActions>
					
					<persistence.storagePid>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:trb_pi_portfolio/Resources/Private/Language/locallang_db.xlf:flex.storage_page</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>3</size>
								<maxitems>50</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
								<wizards>
									<suggest>
										<type>suggest</type>
									</suggest>
								</wizards>
							</config>
						</TCEforms>
					</persistence.storagePid>
				</el>
			</ROOT>
		</sDEF>

		<!-- LIST -->
		<list>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:trb_pi_portfolio/Resources/Private/Language/locallang_db.xlf:flex.list</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.list_layout>
						<TCEforms>
							<label>LLL:EXT:trb_pi_portfolio/Resources/Private/Language/locallang_db.xlf:flex.layout</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:trb_pi_portfolio/Resources/Private/Language/locallang_db.xlf:flex.layout.default</numIndex>
										<numIndex index="1">default</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</settings.list_layout>
				
					<settings.list_filter>
						<TCEforms>
							<label>LLL:EXT:trb_pi_portfolio/Resources/Private/Language/locallang_db.xlf:flex.show_filter</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.list_filter>
				</el>
			</ROOT>
		</list>

		<!-- SLIDER -->
		<slider>
			<ROOT>
