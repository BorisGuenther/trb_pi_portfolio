							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:trb_pi_portfolio/Resources/Private/Language/locallang_db.xlf:flex.layout.default</numIndex>
										<numIndex index="1">default</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</settings.detail_layout>
					
					<settings.pid_detail>
						<TCEforms>
							<label>LLL:EXT:trb_pi_portfolio/Resources/Private/Language/locallang_db.xlf:flex.detail_page</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
