										<numIndex index="1">default</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</settings.slider_layout>
					
					<settings.slider_nav>
						<TCEforms>
							<label>LLL:EXT:trb_pi_portfolio/Resources/Private/Language/locallang_db.xlf:flex.show_navigation</label>
							<config>
								<type>check</type>
							</config>
